<div class="modal-header">
	<h5 class="muted">Enter values:</h5>
</div>
<div class="modal-body">

	<select id="select_list" name="doctor">
    </select>

	<table id="new_schedule">
		<tr>
			<td>
				<strong>Sun: </strong>
			</td>
			<td>
				<input id="sun-start" type="text" size=5 value="<%= schedule["sun"]["start"] %>"> - 
			</td>
			<td>
				<input id="sun-end" type="text" size=5 value="<%= schedule["sun"]["end"] %>">
			</td>
		</tr>
